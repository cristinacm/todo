    <!-- https://w3id.org/todo/tododt#hasSecondaryDialogue -->


    <owl:ObjectProperty rdf:about="https://w3id.org/todo/tododt#hasSecondaryDialogue">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:comment>Establishes the SecondaryDialogue corresponding to a DialogueTrace.</rdfs:comment>
        <rdfs:label>hasSecondaryDialogue</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/todo/tododt#hasSystemRequest -->


    <owl:ObjectProperty rdf:about="https://w3id.org/todo/tododt#hasSystemRequest">
        <rdfs:comment>Establishes the SystemRequest corresponding to a SecondaryDialogueTrace.</rdfs:comment>
        <rdfs:label>hasSystemRequest</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/todo/tododt#hasUserRequest -->


    <owl:ObjectProperty rdf:about="https://w3id.org/todo/tododt#hasUserRequest">
        <rdfs:comment>Establishes the UserRequest corresponding to a DialogueTrace.</rdfs:comment>
        <rdfs:label>hasUserRequest</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/todo/tododt#hasUserResponse -->


    <owl:ObjectProperty rdf:about="https://w3id.org/todo/tododt#hasUserResponse">
        <rdfs:comment>Establishes the UserResponse corresponding to a SecondaryDialogueTrace.</rdfs:comment>
        <rdfs:label>hasUserResponse</rdfs:label>
    </owl:ObjectProperty>
    


