    <!-- https://w3id.org/todo/tododt#ContentUserResponseTrace -->


    <owl:Class rdf:about="https://w3id.org/todo/tododt#ContentUserResponseTrace">
        <rdfs:subClassOf rdf:resource="https://w3id.org/todo/tododt#UserResponseTrace"/>
        <owl:disjointWith rdf:resource="https://w3id.org/todo/tododt#YesNoUserResponseTrace"/>
        <rdfs:comment xml:lang="en">Element that leaves a record of the User Responses performed throughout the dialogue process. These responses provide specific information, as a response for a ContentRequest.</rdfs:comment>
        <rdfs:label>ContentUserResponseTrace</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
        <skos:example xml:lang="en">Content User Response: The error code is 412.</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/todo/tododt#DialogueProcessTrace -->


    <owl:Class rdf:about="https://w3id.org/todo/tododt#DialogueProcessTrace">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/todo/tododt#hasSecondaryDialogue"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/todo/tododt#SecondaryDialogueTrace"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/todo/tododt#dialStatus"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange>
                    <rdfs:Datatype>
                        <owl:oneOf>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>close</rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                        <rdf:first>open</rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </owl:oneOf>
                    </rdfs:Datatype>
                </owl:onDataRange>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/todo/tododt#finishDateTime"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/todo/tododt#initDateTime"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Element that leaves a record of the dialogues performed throughout the dialogue process.</rdfs:comment>
        <rdfs:label>DialogueProcessTrace</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/todo/tododt#DialogueTrace -->


    <owl:Class rdf:about="https://w3id.org/todo/tododt#DialogueTrace">
        <rdfs:subClassOf rdf:resource="https://w3id.org/todo/tododt#DialogueProcessTrace"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/todo/tododt#hasUserRequest"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/todo/tododt#UserRequestTrace"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/todo/tododt#dialogueStep"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Element that leaves a record of the dialogues performed.</rdfs:comment>
        <rdfs:label>DialogueTrace</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/todo/tododt#NoUserResponse -->


    <owl:Class rdf:about="https://w3id.org/todo/tododt#NoUserResponse">
        <rdfs:subClassOf rdf:resource="https://w3id.org/todo/tododt#YesNoUserResponseTrace"/>
        <rdfs:comment xml:lang="en">Element that leaves a record of the User Responses performed throughout the dialogue process. These responses correspond to a negative polarity.</rdfs:comment>
        <rdfs:label>NoUserResponse</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
        <skos:example xml:lang="en">No User Responses: No, Never, Not at all</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/todo/tododt#NullUserResponseTrace -->


    <owl:Class rdf:about="https://w3id.org/todo/tododt#NullUserResponseTrace">
        <rdfs:subClassOf rdf:resource="https://w3id.org/todo/tododt#UserResponseTrace"/>
        <rdfs:comment xml:lang="en">Response that does not represent a value that can be classified as ContentResponse or YesNoResponse.</rdfs:comment>
        <rdfs:label>NullUserResponseTrace</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
        <skos:example xml:lang="en">Null User Response: I don&apos;t know.</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/todo/tododt#SecondaryDialogueTrace -->


    <owl:Class rdf:about="https://w3id.org/todo/tododt#SecondaryDialogueTrace">
        <rdfs:subClassOf rdf:resource="https://w3id.org/todo/tododt#DialogueProcessTrace"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/todo/tododt#hasSystemRequest"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/todo/tododt#SystemRequestTrace"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/todo/tododt#hasUserResponse"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/todo/tododt#UserResponseTrace"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/todo/tododt#text"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Element that leaves a record of the secondary dialogues performed.</rdfs:comment>
        <rdfs:label>SecondaryDialogueTrace</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/todo/tododt#SystemRequestTrace -->


    <owl:Class rdf:about="https://w3id.org/todo/tododt#SystemRequestTrace">
        <rdfs:subClassOf rdf:resource="https://w3id.org/todo/tododt#SystemStepTrace"/>
        <rdfs:comment>Element that leaves a record of the SystemRequests performed throughout the dialogue process.</rdfs:comment>
        <rdfs:label>SystemRequestTrace</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/todo/tododt#SystemResponseTrace -->


    <owl:Class rdf:about="https://w3id.org/todo/tododt#SystemResponseTrace">
        <rdfs:subClassOf rdf:resource="https://w3id.org/todo/tododt#SystemStepTrace"/>
        <rdfs:comment>Element that leaves a record of the SystemResponses performed throughout the dialogue process.</rdfs:comment>
        <rdfs:label>SystemResponseTrace</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/todo/tododt#SystemStepTrace -->


    <owl:Class rdf:about="https://w3id.org/todo/tododt#SystemStepTrace">
        <rdfs:comment>Element that leaves a record of the SystemSteps performed throughout the dialogue process.</rdfs:comment>
        <rdfs:label>SystemStepTrace</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/todo/tododt#UserRequestTrace -->


    <owl:Class rdf:about="https://w3id.org/todo/tododt#UserRequestTrace">
        <rdfs:subClassOf rdf:resource="https://w3id.org/todo/tododt#UserStepTrace"/>
        <owl:disjointWith rdf:resource="https://w3id.org/todo/tododt#UserResponseTrace"/>
        <rdfs:comment xml:lang="en">Element that leaves a record of the User Requests performed throughout the dialogue process.</rdfs:comment>
        <rdfs:label>UserRequestTrace</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/todo/tododt#UserResponseTrace -->


    <owl:Class rdf:about="https://w3id.org/todo/tododt#UserResponseTrace">
        <rdfs:subClassOf rdf:resource="https://w3id.org/todo/tododt#UserStepTrace"/>
        <rdfs:comment xml:lang="en">Element that leaves a record of the User Responses performed throughout the dialogue process.</rdfs:comment>
        <rdfs:label>UserResponseTrace</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/todo/tododt#UserStepTrace -->


    <owl:Class rdf:about="https://w3id.org/todo/tododt#UserStepTrace">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/todo/tododt#text"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Element that leaves a record of the user steps (requests and responses) performed throughout the dialogue process.</rdfs:comment>
        <rdfs:label>UserStepTrace</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
    </owl:Class>
    


    <!-- https://w3id.org/todo/tododt#YesNoUserResponseTrace -->


    <owl:Class rdf:about="https://w3id.org/todo/tododt#YesNoUserResponseTrace">
        <rdfs:subClassOf rdf:resource="https://w3id.org/todo/tododt#UserResponseTrace"/>
        <rdfs:comment xml:lang="en">Element that leaves a record of the User Responses performed throughout the dialogue process. These responses correspond to a positive or negative polarity.</rdfs:comment>
        <rdfs:label>YesNoUserResponseTrace</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
        <skos:example xml:lang="en">YesNo User Responses:

- Yes, OK

- No, Not at all</skos:example>
    </owl:Class>
    


    <!-- https://w3id.org/todo/tododt#YesUserResponse -->


    <owl:Class rdf:about="https://w3id.org/todo/tododt#YesUserResponse">
        <rdfs:subClassOf rdf:resource="https://w3id.org/todo/tododt#YesNoUserResponseTrace"/>
        <rdfs:comment xml:lang="en">Element that leaves a record of the User Responses performed throughout the dialogue process. These responses correspond to a positive polarity.</rdfs:comment>
        <rdfs:label>YesUserResponse</rdfs:label>
        <ns1:term_status>stable</ns1:term_status>
        <skos:example xml:lang="en">Yes User Responses: Yes, OK</skos:example>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // General axioms
    //
    ///////////////////////////////////////////////////////////////////////////////////////
