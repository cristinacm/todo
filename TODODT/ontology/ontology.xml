    <!-- https://w3id.org/todo/tododt#currentValue -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/todo/tododt#currentValue">
        <rdfs:comment>When asking the user for a specific information, the information currently asked to the user.</rdfs:comment>
        <rdfs:label>currentValue</rdfs:label>
        <skos:example xml:lang="en">UserReq: &quot;I want to go to the room&quot;

possibleValue &quot;room1&quot;
possibleValue &quot;room2&quot;

SystemReq: &quot;Do you want to go to {room1}?&quot; (currentValue: room1); &quot;Do you want to go to {room2}? (currentValue: room2)&quot;</skos:example>
        <skos:example xml:lang="en">UserReq: &quot;Quiero ir a la sala&quot;

possibleValue &quot;sala1&quot;
possibleValue &quot;sala2&quot;

System Req: &quot;¿Quieres ir a la {sala1}?&quot; (currentValue: sala1); &quot;¿Quieres ir a la {sala2}? (currentValue: sala2)&quot;</skos:example>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/todo/tododt#dateTime -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/todo/tododt#dateTime">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>Sets a timestamp to a DialogueTrace.</rdfs:comment>
        <rdfs:label>dateTime</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/todo/tododt#dialStatus -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/todo/tododt#dialStatus">
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                        <rdf:first>closed</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#List"/>
                                <rdf:first>open</rdf:first>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>Sets whether a Dialogue/SecondaryDialogue has finished (closed) or not (open).</rdfs:comment>
        <rdfs:label>dialStatus</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/todo/tododt#dialogueStep -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/todo/tododt#dialogueStep">
        <rdfs:comment>Used to control which step is performing the current Dialogue.</rdfs:comment>
        <rdfs:label>dialogueStep</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/todo/tododt#finishDateTime -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/todo/tododt#finishDateTime">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/todo/tododt#dateTime"/>
        <rdfs:comment>Sets a finish timestamp to a DialogueTrace.</rdfs:comment>
        <rdfs:label>finishDateTime</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/todo/tododt#initDateTime -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/todo/tododt#initDateTime">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/todo/tododt#dateTime"/>
        <rdfs:comment>Sets a start timestamp to a DialogueTrace.</rdfs:comment>
        <rdfs:label>initDateTime</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/todo/tododt#possibleValue -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/todo/tododt#possibleValue">
        <rdfs:comment>When asking the user for a specific information, the possible values of that information to be asked to the user.</rdfs:comment>
        <rdfs:label>possibleValue</rdfs:label>
        <skos:example xml:lang="en">UserReq: &quot;I want to go to the room&quot;

possibleValue &quot;room1&quot;
possibleValue &quot;room2&quot;

SystemReq: &quot;Do you want to go to {room1}?&quot;; &quot;Do you want to go to {room2}?&quot;</skos:example>
        <skos:example xml:lang="es">UserReq: &quot;Quiero ir a la sala&quot;

possibleValue &quot;sala1&quot;
possibleValue &quot;sala2&quot;

System Req: &quot;¿Quieres ir a la {sala1}?&quot;; &quot;¿Quieres ir a la {sala2}?&quot;</skos:example>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/todo/tododt#text -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/todo/tododt#text">
        <rdfs:comment>String that corresponds to a transcribed command by an user or the fully constructed outputSentence directed to the user.</rdfs:comment>
        <rdfs:label>text</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/todo/tododt#verb -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/todo/tododt#verb">
        <rdfs:comment>Key element classified as &quot;verb&quot;/&quot;action&quot;.</rdfs:comment>
        <rdfs:label>verb</rdfs:label>
    </owl:DatatypeProperty>
    


