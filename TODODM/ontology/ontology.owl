    <!-- https://w3id.org/todo/tododm#hasStepFunction -->

    <owl:ObjectProperty rdf:about="https://w3id.org/todo/tododm#hasStepFunction">
        <owl:inverseOf rdf:resource="https://w3id.org/todo/tododm#isStepFunctionOf"/>
        <rdfs:comment>Establishes the StepFunction corresponding to a SystemRequest or a ProcessStep.</rdfs:comment>
        <rdfs:isDefinedBy>https://w3id.org/todo/tododm#</rdfs:isDefinedBy>
        <rdfs:label>hasStepFunction</rdfs:label>
        <vs:term_status>stable</vs:term_status>
    </owl:ObjectProperty>
    


